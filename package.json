  "repository": {
    "type": "git",
    "url": "git+https://github.com/tanishq-singh-2301/react-videotape"
  },
  "keywords": [
    "react",
    "typescript",
    "videoplayer",
    "videotape",
    "video-player",
    "video"
  ],
  "bugs": {

{
  "name": "react-videotape",
  "version": "1.1.14",
  "description": "Video player with build in controls for react",
  "author": "Tanishq Singh <tanishqsingh640@gmail.com>",
  "contributors": [],
  "repository": "git+https://github.com/tanishq-singh-2301/react-videotape",
  "license": "MIT",
  "main": "build/lib/cjs/index.js",
  "module": "build/lib/esm/index.js",
  "files": [
    "build/lib"
  ],
  "scripts": {
    "pretty": "prettier --write src && prettier --write web",
    "build": "rm -rf build/ && yarn pretty && yarn build:lib && yarn build:web",
    "dev": "rm -rf build/ && concurrently \"yarn watch:lib\" \"yarn watch:web\" ",
    "serve": "serve -s build/web",
    "build:lib": "rm -rf build/lib && yarn build:lib:esm && yarn build:lib:cjs && yarn build:lib:css",
    "build:lib:esm": "tsc",
    "build:lib:cjs": "tsc --module CommonJS --outDir build/lib/cjs",
    "build:lib:css": "tailwindcss -c ./tailwind.config.js -i ./styles/global.css -o ./build/lib/index.css --minify",
    "watch:lib": "concurrently \"yarn watch:lib:esm\" \"yarn watch:lib:cjs\" \"yarn watch:lib:css\" ",
    "watch:lib:esm": "tsc --watch",
    "watch:lib:cjs": "tsc --module CommonJS --outDir build/lib/cjs --watch",
    "watch:lib:css": "tailwindcss -c ./tailwind.config.js -i ./styles/global.css -o ./build/lib/index.css --watch",
    "build:web": "rm -rf build/web && yarn build:web:js",
    "build:web:js": "NODE_ENV=production webpack --config ./webpack/webpack.config.js",
    "watch:web": "yarn watch:web:js",
    "watch:web:js": "webpack serve --hot --config ./webpack/webpack.config.js"
  },
  "bugs": {
    "url": "https://github.com/tanishq-singh-2301/react-videotape/issues"
  },
  "homepage": "https://github.com/tanishq-singh-2301/react-videotape#readme",
  "keywords": [
    "react",
    "typescript",
    "videoplayer",
    "videotape",
    "video-player",
    "video"
  ],
  "dependencies": {
    "moment-duration-format": "^2.3.2",
    "moment-timezone": "^0.5.34",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-full-screen": "^1.1.1",
    "react-icons": "^4.4.0",
    "react-slid": "^1.0.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.13",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@tailwindcss/jit": "^0.1.18",
    "@types/moment-duration-format": "^2.2.3",
    "@types/node": "^17.0.21",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/tailwindcss": "^3.1.0",
    "autoprefixer": "^10.4.8",
    "babel-loader": "^8.2.5",
    "concurrently": "^7.3.0",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "file-loader": "^6.2.0",
    "html-loader": "^4.1.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.1",
    "postcss": "^8.4.16",
    "postcss-loader": "^7.0.1",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-refresh": "^0.14.0",
    "react-router-dom": "^6.3.0",
    "sass": "^1.54.5",
    "sass-loader": "^13.0.2",
    "serve": "^14.0.1",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.1.8",
    "terser-webpack-plugin": "^5.3.5",
    "typescript": "^4.5.2",
    "webpack": "^5.74.0",
    "webpack-bundle-analyzer": "^4.6.1",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.10.0"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
